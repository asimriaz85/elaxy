		while($row = mysql_fetch_array($result))
		{
			
			$post_id=$row['id'];
			
			$published_date=$row['date'];
			$town=$row['town'];
			
			
			$daysago = (strtotime($cdate) - strtotime($published_date)) / (60 * 60 * 24);
			
